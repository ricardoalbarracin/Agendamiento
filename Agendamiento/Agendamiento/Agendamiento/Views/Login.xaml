<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
               xmlns:local="clr-namespace:Agendamiento;assembly=Agendamiento"
             x:Class="Agendamiento.Views.Login">
    <StackLayout Padding="10,30,10,10" Spacing="10" >
      <Label
           Text="Usuario"
           VerticalOptions="Center"
           HorizontalOptions="Start"
           />
    <Entry x:Name="UbbserEntry" Placeholder="Email"></Entry>
    
    
      <Entry x:Name="PasswordEntry"
           Placeholder="Contraseña">
    </Entry>
    <Button x:Name="EnterButton" Text="Ingresar" Clicked="enterButton_Clicked" TextColor="White" BackgroundColor ="#2196F3" ></Button>
  </StackLayout>
</ContentPage>